@model IEnumerable<HWPCBug.Data.Ticket>
@{
    ViewBag.Title = "Home Page";
    ViewBag.InitModule = "homeIndex";
}
@section Scripts {
<script src="~/js/home-index.js"></script>
}

<div data-ng-view=""><div class="ng-scope">
    <div class="ticket-header">
        <h1>Tickets</h1>
        <p class="lead">Latest Tickets...</p>
        <div class="ng-binding">Ticket Count: 6</div>
    </div>

    <div>
        <a href="#/newticket">Add New Ticket</a>
    </div>

    <div class="progress progress-striped active ng-hide" data-ng-show="isBusy">
        <div class="bar" style="width: 100%;"></div>
    </div>

    <div class="ticket-list">
        <!-- ngRepeat: i in data.tickets --><div class="ticket-box row ng-scope" data-ng-repeat="i in data.tickets">
            <div class="title span12">
                <a data-ng-href="#/ticket/6" class="ng-binding" href="#/ticket/6">A new ticket from the angular interface.</a>
            </div>
            <div class="date span2 ng-binding">
                Jan 13, 2014 1:44:13 AM
            </div>
        </div><!-- end ngRepeat: i in data.tickets --><div class="ticket row ng-scope" data-ng-repeat="i in data.tickets">
            <div class="title span10">
                <a data-ng-href="#/ticket/5" class="ng-binding" href="#/ticket/5">A new ticket from the angular interface.</a>
            </div>
            <div class="date span2 ng-binding">
                Jan 13, 2014 1:41:17 AM
            </div>
        </div><!-- end ngRepeat: i in data.tickets --><div class="ticket row ng-scope" data-ng-repeat="i in data.tickets">
            <div class="title span10">
                <a data-ng-href="#/ticket/4" class="ng-binding" href="#/ticket/4">A new ticket from the angular interface.</a>
            </div>
            <div class="date span2 ng-binding">
                Jan 13, 2014 1:40:24 AM
            </div>
        </div><!-- end ngRepeat: i in data.tickets --><div class="ticket row ng-scope" data-ng-repeat="i in data.tickets">
            <div class="title span10">
                <a data-ng-href="#/ticket/3" class="ng-binding" href="#/ticket/3">Testing Post</a>
            </div>
            <div class="date span2 ng-binding">
                Jan 12, 2014 4:28:54 PM
            </div>
        </div><!-- end ngRepeat: i in data.tickets --><div class="ticket row ng-scope" data-ng-repeat="i in data.tickets">
            <div class="title span10">
                <a data-ng-href="#/ticket/2" class="ng-binding" href="#/ticket/2">Ticket 2 Note Text</a>
            </div>
            <div class="date span2 ng-binding">
                Jan 11, 2014 6:51:31 PM
            </div>
        </div><!-- end ngRepeat: i in data.tickets --><div class="ticket row ng-scope" data-ng-repeat="i in data.tickets">
            <div class="title span10">
                <a data-ng-href="#/ticket/1" class="ng-binding" href="#/ticket/1">Ticket 1 Note Text</a>
            </div>
            <div class="date span2 ng-binding">
                Jan 11, 2014 6:51:31 PM
            </div>
        </div><!-- end ngRepeat: i in data.tickets -->
    </div>
</div>
</div>
